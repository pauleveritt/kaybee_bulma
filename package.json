{
  "name": "kaybee_bulma",
  "version": "0.0.1",
  "description": "Kaybee theme based on Bulma CSS",
  "main": "kaybee_bulma/index.js",
  "scripts": {
    "start": "parcel ./kaybee_bulma/index.html",
    "test": "jest --coverage --no-cache",
    "json": "json-server --watch db.json",
    "build": "parcel build -d kaybee_bulma/static -o kaybee_bulma kaybee_bulma/index.tsx"

  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pauleveritt/kaybee.git"
  },
  "author": "Paul Everitt <pauleveritt@me.com>",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/pauleveritt/kaybee/issues"
  },
  "homepage": "https://github.com/pauleveritt/kaybee#readme",
  "dependencies": {
    "bulma": "^0.7.1",
    "bulma-steps-component": "^0.5.3",
    "hyperapp": "^1.2.5"
  },
  "devDependencies": {
    "@types/jest": "^22.2.2",
    "jest": "^22.4.3",
    "json-server": "^0.12.2",
    "node-sass": "^4.9.0",
    "parcel-bundler": "^1.6.2",
    "ts-jest": "^22.4.2",
    "tslint": "^5.9.1",
    "typescript": "^2.7.2"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
